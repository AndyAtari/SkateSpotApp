<h1>New Users Signup!</h1>
<%= form_for @user do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :password %>
  <%= f.text_field :password %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :city %> 
  <%= f.text_field :city %> 
  <%= f.submit "Sign Me Up"%>
  <% end %>
  